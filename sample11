	if (condition){
		statement;		//true
	}else {
		statement;		//false
	}
*/
#include <iostream>
using namespace std;
int main(){
	int x;
	
	cout << "Enter a number: ";
	cin >> x;
	if (x>=100){
		cout << "Number is greater than 100";
	}else {
		cout << "Number is less than 100";
	}	
	return 0;
}




