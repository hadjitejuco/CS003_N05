//if else
/*
	Enter Grade 0-100 - 100 -> A
	Range		Grade
	80 to 100	A+
	70 to 79	A
	60 to 69	B
	40 to 59    C
	0 to 40 	F
*/
#include <iostream>
using namespace std;
int main(){
	int g;
	cout << " Enter grade " ;
	cin >> g;
	if (g>=80){
		cout << "Grade = A+";
	}else{
		if (g>=70){
			cout << "Grade = A";
		}
		else{
			if (g>=60){
				cout << "Grade = B";
			}else{
				if (g>=40){
					cout << "Grade = C";
				}else{
					cout << "Grade = F";
				}
			}
		}
	}
	
	
	return 0;
}
